    "build-sql": "ts-node-dev --respawn ./mysql/creator.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Travis-hjs/node-koa.git"
  },
  "keywords": [
    "koa",
    "nodejs"
  ],
  "author": "Travis-hjs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Travis-hjs/node-koa/issues"
  },
  "homepage": "https://github.com/Travis-hjs/node-koa#readme",
  "dependencies": {
    "koa": "2.15.3",
    "koa-body": "4.2.0",
    "koa-router": "10.1.1",
    "koa-static": "5.0.0",
    "mysql": "2.18.1"
  },
  "devDependencies": {
    "@types/koa": "2.15.0",
    "@types/koa-router": "7.4.8",
    "@types/koa-static": "4.0.4",
    "@types/mysql": "2.15.26",
    "ts-node": "9.1.1",
    "ts-node-dev": "2.0.0",
    "typescript": "4.9.5"
